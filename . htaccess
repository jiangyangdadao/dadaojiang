# Enable custom routing.
RewriteEngine On


# Route documentation and tutorial pages.
RewriteRule ^(documentation|tutorials)\/.*$ GTUIKit.doccarchive/index.html [L]


RewriteRule ^(css|js|data|images|downloads|favicon\.ico|favicon\.svg|img|theme-settings\.json|videos)\/.*$ GTUIKit.doccarchive/$0 [L]
